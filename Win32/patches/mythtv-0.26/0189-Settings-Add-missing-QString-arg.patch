---
 mythtv/libs/libmyth/settings.cpp |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/mythtv/libs/libmyth/settings.cpp b/mythtv/libs/libmyth/settings.cpp
index 6a1a539..5b85349 100644
--- a/mythtv/libs/libmyth/settings.cpp
+++ b/mythtv/libs/libmyth/settings.cpp
@@ -200,7 +200,7 @@ void SelectSetting::setValue(int which)
     if ((which >= ((int) values.size())) || (which < 0))
     {
         LOG(VB_GENERAL, LOG_ERR,
-                 QString("SelectSetting::setValue(): invalid index: %1 size: ")
+                 QString("SelectSetting::setValue(): invalid index: %1 size: %2")
                      .arg(which).arg(values.size()));
     }
     else
